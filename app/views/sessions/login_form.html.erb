<h2>Welcome to GenShout!</h2>

<% if flash[:message] %>
    <div><%= flash[:message] %></div>
<% end %>

<%= form_with url: login_path do |f| %>
    <%= f.label :username %>
    <%= f.text_field :username %>
    <%= f.label :password %>
    <%= f.password_field :password %>
    <%= f.submit "Login" %>
<% end %>